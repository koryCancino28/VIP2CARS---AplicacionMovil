{
  "expo": {
    "name": "Vip2cars",
    "slug": "Vip2cars",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/logo-cuadrado.png",
    "scheme": "Vip2cars",

    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {

        "foregroundImage": "./assets/images/logo-cuadrado.png",
        "backgroundColor": "#000000"

      },
      "edgeToEdgeEnabled": true,
      "package": "com.jerry29.vip2cars"
    },
    "web": {
      "bundler": "metro",
      "output": "static",

      "favicon": "./assets/images/logo-cuadrado.png"

    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-vip2cars.png",
          "imageWidth": 500,
          "resizeMode": "center",
          "backgroundColor": "#000000"

        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3fb01331-7856-4ad7-8164-b7ab8c9855da"
      }
    }
  }
}
